@article{Marco-Ruiz2015a,
   author = {Luis Marco-Ruiz and David Moner and José A. Maldonado and Nils Kolstrup and Johan G. Bellika},
   doi = {10.1016/j.ijmedinf.2015.05.016},
   issn = {13865056},
   issue = {9},
   journal = {International Journal of Medical Informatics},
   month = {9},
   note = {Antecedentes: La reutilización de los datos capturados durante la atención de la salud es esencial para satisfacer las demandas de la investigación clínica y los sistemas de apoyo a la toma de decisiones clínicas. Una barrera principal para la reutilización es la existencia de formatos heredados de datos y la alta granularidad de la misma cuando se almacena en un sistema de registro de salud electrónico (DSE). Por lo tanto, necesitamos mecanismos para estandarizar, agrupar y consultar datos ocultos en los DSE, para permitir su reutilización cada vez que se necesiten.<br/><br/>
   Materiales y métodos: El trabajo presentado hace uso de las mejores tecnologías de transformación y almacenamiento de datos basadas en el arquetipo para crear un flujo de trabajo para el modelado, extracción, transformación y carga de datos propietarios de EHR en repositorios de datos estandarizados. Hemos convertido los datos legados y realizado el paciente centrado en las agregaciones a través de arquetipo basado en transformaciones. Posteriormente, se realizaron agregaciones de propósito específico a un nivel de consulta para casos de uso particulares.<br/><br/>Resultados: Se han normalizado los resultados de las pruebas de laboratorio de una población de 230.000 pacientes pertenecientes a los condados de Troms y Finnmark en Noruega solicitados entre enero de 2013 y noviembre de 2014. La normalización de registros de prueba se ha realizado mediante la definición de funciones de transformación y agregación entre los registros de laboratorio y un arquetipo. Estas asignaciones se utilizaron para generar automáticamente datos compatibles con EHR abiertos. Estos datos se cargaron en un almacén de datos basado en un arquetipo. Una vez cargado, definimos indicadores relacionados con los datos en el almacén para monitorear la actividad de prueba de Salmonella y Pertussis usando el lenguaje de consulta de arquetipo.<br/><br/>Discusión: Las normas y tecnologías basadas en el arquetipo se pueden utilizar para crear un entorno de almacén de datos que permita que los datos de los sistemas de RSE sean reutilizados en la investigación clínica y en los sistemas de apoyo a las decisiones. Con este enfoque, los datos de EHR existentes se hacen disponibles en un formato estandarizado e interoperable, abriendo así un mundo de posibilidades hacia la reutilización semántica o basada en concepto, la consulta y la comunicación de datos clínicos. (C) 2015 Elsevier Ireland Ltd. Todos los derechos reservados},
   pages = {702-714},
   title = {Archetype-based data warehouse environment to enable the reuse of electronic health record data},
   volume = {84},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S1386505615300058},
   year = {2015},
}
@article{Spath2011,
   author = {Melanie Bettina Späth and Jane Grimson},
   doi = {10.1016/j.ijmedinf.2010.11.002},
   issn = {13865056},
   issue = {3},
   journal = {International Journal of Medical Informatics},
   month = {3},
   note = {Algunos arquetipos<br/>existentes se reutilizaron sin cambios, algunos se modificaron o se<br/>especializaron, y se desarrollaron nuevos arquetipos donde fue necesario. Los<br/>campos del esquema de la base de datos del biobanco fueron entonces mapeados a<br/>los elementos de los arquetipos. Por último, los arquetipos se organizaron en<br/>plantillas específicamente para satisfacer los requisitos del biobanco de PCRC.<br/>Resultados: Se encontró un conjunto de 47 arquetipos que cubren todos los<br/>conceptos utilizados en el biobanco. De éstos, 29 (62%) se reutilizaron sin<br/>cambios, 6 se modificaron y / o ampliaron, 1 se especializó y 11 se definieron<br/>de nuevo. Estos arquetipos se organizaron en 8 plantillas específicamente<br/>requeridas para este biobanco. En esta investigación se encontraron varios<br/>problemas. Algunos surgieron de la inmadurez del enfoque del arquetipo, como<br/>las herramientas de apoyo al modelado inmaduro, las dificultades para definir<br/>arquetipos de alta calidad y el problema de los arquetipos superpuestos.<br/>Además, la identificación de adecuados arquetipos existentes fue mucho tiempo y<br/>muchos conflictos semánticos se encontraron durante el proceso de mapeo de la<br/>base de datos BIMS PCRC a arquetipos existentes. Estos incluyen diferencias en<br/>la granularidad de la documentación, en el nivel de metadatos versus el nivel<br/>de datos de modelado, en terminologías y vocabularios utilizados, y en la<br/>cantidad de estructura impuesta sobre la información que se va a registrar.<br/>Además, se encontró que la forma actual de modelar la entidad de la muestra era<br/>engorrosa en la actividad centrada en la muestra del biobanco.<br/><br/><br/> <br/><br/>El enfoque del arquetipo es un enfoque prometedor para crear un eBMRR compartible basado en el participante del estudio / donante para los biobancos. Muchos arquetipos originalmente desarrollados para el dominio de EHR pueden ser reutilizados para modelar la información clínica / fenotípica y de muestra en el contexto del biobanco, lo que valida la genericidad de estos arquetipos y su potencial de reutilización en el contexto de la investigación biomédica. Sin embargo, encontrar arquetipos adecuados en los repositorios y establecer un mapeo exacto entre los campos de la base de datos BIMS del PCRC y los elementos de los arquetipos existentes que han sido diseñados para la práctica clínica puede ser desafiante y requiere mucho tiempo e implica resolver muchos conflictos comunes de integración de sistemas. Estos pueden ser atribuibles a las diferencias en los requisitos para la documentación de la información entre la práctica clínica y biobanking. Esta investigación también reconoció la necesidad de mejores herramientas de apoyo, directrices de modelización y mejores prácticas y reconfirmó la necesidad de una mejor gobernanza del conocimiento de dominio. Además, los autores proponen que se investigue el establecimiento de un registro de muestra independiente con la muestra como sujeto discográfico. La investigación presentada en este trabajo está limitada por el hecho de que los nuevos arquetipos desarrollados durante esta investigación se basan en un único caso de biobanco. Estos nuevos arquetipos pueden no estar completos, representando sólo aquellos subconjuntos de elementos requeridos por esta base de datos particular. Sin embargo, este ejercicio expone algunas de las brechas que existen en el paisaje arquetípico y pone de relieve los conceptos que necesitan ser modelados con arquetipos para permitir el desarrollo de un eBMRR. (C) 2010 Elsevier Ireland Ltd<br/><br/><br/><br/><br/> <br/><br/><br/><br/><br/><br/>},
   pages = {205-226},
   title = {Applying the archetype approach to the database of a biobank information management system},
   volume = {80},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S1386505610002145},
   year = {2011},
}
@article{Sari2012,
   author = {Anny Kartika Sari and Wenny Rahayu and Mehul Bhatt},
   doi = {10.1016/j.knosys.2011.07.004},
   issn = {09507051},
   journal = {Knowledge-Based Systems},
   month = {2},
   note = {sari2012<br>
    	El esfuerzo global en la estandarización de los registros de salud electrónicos ha impulsado la necesidad de un modelo que permita a los médicos interactuar con el sistema de información médica recién estandarizado centrándose en los conceptos / procesos médicos reales en lugar de las representaciones de datos subyacentes. Se ha introducido un arquetipo como modelo que representa conceptos o procesos de salud funcional como el registro de admisión, lo que permite capturar toda la información relevante para los procesos de forma transparente para los usuarios. Sin embargo, es necesario asegurar que los arquetipos capten con precisión toda la información relevante a los conceptos del arquetipo. Por lo tanto, una columna vertebral semántica es necesaria para cada uno de los arquetipos.<br><br>En este artículo proponemos el desarrollo de una sub-ontología de arquetipo para cada arquetipo que represente el contenido semántico del correspondiente arquetipo. La sub-ontología se extrae semiautomáticamente de una ontología estándar de salud, en este caso SNOMED CT. Dos pasos realizados para construir una sub-ontología de arquetipo son el proceso de anotación y el proceso de extracción, en el que se deben aplicar algunas reglas para mantener la validez de la sub-ontología. El enfoque se evalúa utilizando las sub-ontologías de los arquetipos producidas en el desarrollo de un nuevo arquetipo para garantizar que sólo los arquetipos relevantes pueden estar vinculados al arquetipo que se está desarrollando, de modo que los únicos datos relevantes se capturan usando el arquetipo particular. Se muestra que el método produce mejores resultados que el enfoque actual en el que no se utiliza una sub-ontología de arquetipo. Concluimos que la sub-ontología del arquetipo puede representar bien el contenido semántico del arquetipo. (C) 2011 Elsevier B.V. Todos los derechos reservados.},
   pages = {75-85},
   title = {Archetype sub-ontology: Improving constraint-based clinical knowledge model in electronic health records},
   volume = {26},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S0950705111001444},
   year = {2012},
}
@article{Govinda2015,
   abstract = {<p>Cloud computing has become a victorious archetype for data storage, as well as for computation purposes. Greater than ever it concerns user’s privacy, so that data security in a cloud is increasing day by day. Ensuring security and privacy for data organization and query dispensation in the cloud is important for superior and extended uses of cloud based technologies. Cloud users can barely have the full benefits of cloud computing if we can ensure the real user’s privacy and his data security concerns this approach along with storing thin-skinned personal information in databases and software spread around the cloud. There are numerous service suppliers in WWW (World Wide Web), who can supply each service as a cloud. These cloud services will switch over data with a supplementary cloud, so that when the data is exchanged between the clouds, the problem of confidentiality revelation exists. So the privacy revelation problem concerning a person or a corporation is unavoidably open when releasing or data distributing in the cloud service. Confidentiality is a significant issue in any cloud computing environment. In this paper we propose and implement a mechanism to maintain privacy and secure data storage for group members or a community in cloud environment.</p>},
   author = {K. Govinda and E. Sathiyamoorthy},
   doi = {10.1515/cait-2015-0005},
   issn = {1314-4081},
   issue = {1},
   journal = {Cybernetics and Information Technologies},
   month = {1},
   note = {10.1515@cait-2015-0005<br/><br/>La computación en nube se ha convertido en un arquetipo victorioso para el almacenamiento de datos, así como para fines de cálculo. Mayor que nunca, se refiere a la privacidad del usuario, de modo que la seguridad de los datos en una nube está aumentando día a día. Garantizar la seguridad y privacidad para la organización de datos y la dispensación de consultas en la nube es importante para los usos superiores y extendidos de las tecnologías basadas en la nube. Los usuarios de la nube apenas pueden tener todos los beneficios de la computación en la nube si podemos garantizar la privacidad del usuario real y sus preocupaciones de seguridad de datos, junto con el almacenamiento de información personal en bases de datos y software distribuidos por la nube. Hay numerosos proveedores de servicios en WWW (World Wide Web), que pueden suministrar cada servicio como una nube. Estos servicios de nube cambiarán los datos con una nube complementaria, de modo que cuando los datos se intercambien entre las nubes, el problema de la revelación de confidencialidad existe. Por lo tanto, el problema de la revelación de la privacidad con respecto a una persona o una corporación es inevitablemente abierto al liberar o distribuir datos en el servicio en la nube. La confidencialidad es un problema importante en cualquier entorno de cloud computing. En este artículo proponemos e implementamos un mecanismo para mantener la privacidad y el almacenamiento seguro de datos para los miembros del grupo o una comunidad en el entorno de la nube.},
   title = {Privacy Preservation of a Group and Secure Data Storage in Cloud Environment},
   volume = {15},
   url = {https://www.degruyter.com/view/j/cait.2015.15.issue-1/cait-2015-0005/cait-2015-0005.xml},
   year = {2015},
}
@article{Kopanitsa2015,
   author = {Georgy Kopanitsa and Hasan Veseli and Vladimir Yampolsky},
   doi = {10.1016/j.jbi.2015.04.009},
   issn = {15320464},
   journal = {Journal of Biomedical Informatics},
   month = {6},
   note = {kopanitsa2015<br/><br/>Antecedentes: Cuando los datos médicos se han registrado o intercambiado satisfactoriamente entre sistemas, parece necesario presentar los datos de forma coherente para asegurarse de que se entienda e interprete claramente. Una interfaz de usuario basada en estándares puede proporcionar interoperabilidad en el nivel visual.<br/><br/>Objetivos: El objetivo de esta investigación fue desarrollar, implementar y evaluar un modelo de información para construir interfaces de usuario para los datos médicos basados ​​en el arquetipo.<br/><br/>Métodos: Se identificaron los siguientes tipos de conocimiento como elementos importantes y se incluyeron en el modelo de información: atributos relacionados con el contenido médico, atributos relacionados con el tipo de datos, atributos relacionados con el usuario, atributos relacionados con el dispositivo. Para apoyar interfaces de usuario flexibles y eficientes se eligió un enfoque que representa diferentes tipos de conocimiento con diferentes modelos que separan el concepto médico de un concepto visual y la realización de interfaces. Evaluamos el enfoque desarrollado utilizando la Guía para Buenas Prácticas de Evaluación en Informática de la Salud (GEP-HI).<br/><br/>Resultados: Se desarrolló un modelo de información de nivel superior para complementar el modelo de arquetipo ISO 13606. Esto permitió la especificación de las propiedades de presentación en el momento de la definición de los arquetipos. El modelo permite realizar diferentes perspectivas de los usuarios sobre los datos. El enfoque fue implementado y evaluado dentro de un sistema EHR en funcionamiento. La evaluación incluyó a 30 pacientes de diferente edad y experiencia en TI y 5 médicos. Un mes de pruebas demostró que el tiempo necesario para leer los registros de salud electrónicos disminuyó para ambos médicos (desde un promedio de 310 a 220 s) y para los pacientes (de un promedio de 95 a 39 s). Los usuarios informaron un alto nivel de satisfacción y motivación para usar el enfoque de visualización de datos presentado especialmente en comparación con su experiencia previa.<br/><br/>Conclusión: El modelo de información introducido permite separar el conocimiento médico y el conocimiento de la presentación. La capa de presentación adicional enriquecerá la flexibilidad de la interfaz gráfica del usuario y permitirá una presentación óptima de los datos médicos teniendo en cuenta las perspectivas de los diferentes usuarios y los diferentes medios utilizados para la presentación de los datos. (C) 2015 Elsevier Inc. Todos los derechos reservados.<br/>Palabra clave},
   pages = {196-205},
   title = {Development, implementation and evaluation of an information model for archetype based user responsive medical data visualization},
   volume = {55},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S1532046415000763},
   year = {2015},
}
@article{Marco-Ruiz2015,
   author = {Luis Marco-Ruiz and David Moner and José A. Maldonado and Nils Kolstrup and Johan G. Bellika},
   doi = {10.1016/j.ijmedinf.2015.05.016},
   issn = {13865056},
   issue = {9},
   journal = {International Journal of Medical Informatics},
   month = {9},
   note = {marcoruiz2015<br>
    	Antecedentes: La reutilización de los datos capturados durante la atención de la salud es esencial para satisfacer las demandas de la investigación clínica y los sistemas de apoyo a la toma de decisiones clínicas. Una barrera principal para la reutilización es la existencia de formatos heredados de datos y la alta granularidad de la misma cuando se almacena en un sistema de registro de salud electrónico (DSE). Por lo tanto, necesitamos mecanismos para estandarizar, agrupar y consultar datos ocultos en los DSE, para permitir su reutilización cada vez que se necesiten.<br><br>Objetivo: Crear una infraestructura de data warehouse utilizando tecnologías, estándares y lenguajes de consulta del arquetipo para permitir la interoperabilidad necesaria para la reutilización de datos.<br><br>Materiales y métodos: El trabajo presentado hace uso de las mejores tecnologías de transformación y almacenamiento de datos basadas en el arquetipo para crear un flujo de trabajo para el modelado, extracción, transformación y carga de datos propietarios de EHR en repositorios de datos estandarizados. Hemos convertido los datos legados y realizado el paciente centrado en las agregaciones a través de arquetipo basado en transformaciones. Posteriormente, se realizaron agregaciones de propósito específico a un nivel de consulta para casos de uso particulares.<br><br>Resultados: Se han normalizado los resultados de las pruebas de laboratorio de una población de 230.000 pacientes pertenecientes a los condados de Troms y Finnmark en Noruega solicitados entre enero de 2013 y noviembre de 2014. La normalización de registros de prueba se ha realizado mediante la definición de funciones de transformación y agregación entre los registros de laboratorio y un arquetipo. Estas asignaciones se utilizaron para generar automáticamente datos compatibles con EHR abiertos. Estos datos se cargaron en un almacén de datos basado en un arquetipo. Una vez cargado, definimos indicadores relacionados con los datos en el almacén para monitorear la actividad de prueba de Salmonella y Pertussis usando el lenguaje de consulta de arquetipo.<br><br>Discusión: Las normas y tecnologías basadas en el arquetipo se pueden utilizar para crear un entorno de almacén de datos que permita que los datos de los sistemas de RSE sean reutilizados en la investigación clínica y en los sistemas de apoyo a las decisiones. Con este enfoque, los datos de EHR existentes se hacen disponibles en un formato estandarizado e interoperable, abriendo así un mundo de posibilidades hacia la reutilización semántica o basada en concepto, la consulta y la comunicación de datos clínicos. (C) 2015 Elsevier Ireland Ltd. Todos los derechos reservados},
   pages = {702-714},
   title = {Archetype-based data warehouse environment to enable the reuse of electronic health record data},
   volume = {84},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S1386505615300058},
   year = {2015},
}
@article{Epifanio2016,
   author = {Irene Epifanio},
   doi = {10.1016/j.csda.2016.06.007},
   issn = {01679473},
   journal = {Computational Statistics & Data Analysis},
   month = {12},
   note = {epifanio2016<br>El análisis de arquetipos y arquetipos puede extenderse a los datos funcionales. Cada función se aproxima mediante una combinación convexa de observaciones reales (arquetipos funcionales) o arquetipos funcionales, que son una combinación convexa de observaciones en el conjunto de datos. Se usan datos de temperatura canadienses conocidos para ilustrar el análisis desarrollado. Se proponen métodos computacionales para realizar estos análisis, basados ​​en los coeficientes de una base. A diferencia de un intento previo de calcular arquétipos funcionales, que sólo era válido para una base ortogonal, la metodología propuesta puede ser utilizada para cualquier base. Es computacionalmente menos exigente que el enfoque simple de discretizar las funciones. El arquetipo funcional multivariado y el análisis de los arquetipos también se introducen y aplican en un interesante problema sobre el estudio del desarrollo humano en todo el mundo durante los últimos 50 años. Estas herramientas pueden contribuir a la comprensión de un conjunto de datos funcionales, como en el caso multivariado clásico. (C) 2016 Elsevier B.V. Todos los derechos reservados.},
   pages = {24-34},
   title = {Functional archetype and archetypoid analysis},
   volume = {104},
   url = {http://linkinghub.elsevier.com/retrieve/pii/S016794731630144X},
   year = {2016},
}
@article{Ragozini2017,
   author = {G. Ragozini and F. Palumbo and M. R. D'Esposito},
   doi = {10.1002/sam.11325},
   issn = {19321864},
   issue = {1},
   journal = {Statistical Analysis and Data Mining: The ASA Data Science Journal},
   month = {2},
   note = {ragozini2016<br>
    	Los prototipos, como Rosch (1973) definieron el término en el campo de las ciencias cognitivas, son ejemplares ideales que resumen y representan grupos de objetos (o categorías) y que son típicos "según su semejanza interna y disimilitud externa frente a otros grupos O categorıas En el marco del enfoque cognitivo se propone un procedimiento basado en datos para la identificación de prototipos que se basa en el análisis arquetípico y el análisis de los datos compositivos El procedimiento aquí presentado explora las propiedades de los arquetipos, tanto en términos de su disimilaridad externa en relación A otros puntos del conjunto de datos y en términos de su capacidad para representar los datos a través de composiciones en un simplex en el que es posible agrupar todas las observaciones El procedimiento propuesto es útil no sólo para los puntos de datos reales habituales, También se utiliza para datos de intervalos, datos funcionales y datos relacionales, y proporciona prototipos bien separados y claramente perfilados. (C) 2016 Wiley Pe Riodicals, Inc. Análisis Estadístico y Minería de Datos: The ASA Data Science Journal, 2016},
   pages = {6-20},
   title = {Archetypal analysis for data-driven prototype identification},
   volume = {10},
   url = {http://doi.wiley.com/10.1002/sam.11325},
   year = {2017},
}
@inproceedings{Safina2016,
   author = {Larisa Safina and Manuel Mazzara and Fabrizio Montesi and Victor Rivera},
   doi = {10.1109/AINA.2016.95},
   isbn = {978-1-5090-1858-1},
   journal = {2016 IEEE 30th International Conference on Advanced Information Networking and Applications (AINA)},
   month = {3},
   note = {safina2016_<br/>Abstracto<br/>Microservices es un estilo arquitectónico inspirado en la informática orientada al servicio que recientemente ha empezado a ganar popularidad. Jolie es un lenguaje de programación basado en el paradigma de los microservicios: el componente principal de los sistemas Jolie son los servicios, en contraste con, por ejemplo, las funciones u objetos. Las primitivas ofrecidas por el lenguaje Jolie provocan muchos de los patrones recurrentes encontrados en microservicios, como equilibradores de carga y procesos estructurados. Sin embargo, Jolie todavía carece de algunas construcciones útiles para tratar con tipos de mensajes y manipulación de datos que están presentes en la informática orientada al servicio. En este artículo nos centramos en la posibilidad de expresar opciones a nivel de tipos de datos, una característica bien representada en estándares para Servicios Web, por ejemplo, WSDL. Extendemos Jolie para soportar este tipo de opciones, y activar los procesos Jolie para actuar sobre los datos genéricamente (sin saber qué tipo tiene en la elección). Demostramos el impacto de nuestra implementación en algunos de los escenarios típicos encontrados en los sistemas de microservicio. Esto muestra cómo el cómputo puede moverse de un enfoque impulsado por el proceso a un enfoque basado en datos, y conduce a la identificación preliminar de patrones de comunicación recurrentes que pueden configurarse como patrones de diseño.},
   pages = {430-437},
   publisher = {IEEE},
   title = {Data-Driven Workflows for Microservices: Genericity in Jolie},
   url = {http://ieeexplore.ieee.org/document/7474121/},
   year = {2016},
}
@inproceedings{Qanbari2016,
   author = {Soheil Qanbari and Samim Pezeshki and Rozita Raisi and Samira Mahdizadeh and Rabee Rahimzadeh and Negar Behinaein and Fada Mahmoudi and Shiva Ayoubzadeh and Parham Fazlali and Keyvan Roshani and Azalia Yaghini and Mozhdeh Amiri and Ashkan Farivarmoheb and Arash Zamani and Schahram Dustdar},
   doi = {10.1109/IoTDI.2015.18},
   isbn = {978-1-4673-9948-7},
   journal = {2016 IEEE First International Conference on Internet-of-Things Design and Implementation (IoTDI)},
   month = {4},
   note = {<br>
    	Abstracto<br>El objetivo de los patrones de diseño es hacer el diseño robusto y abstraer las soluciones reutilizables detrás de las interfaces expresivas, independientemente de una plataforma concreta. Se abstraen de la complejidad de las tecnologías subyacentes y habilitadoras. Las cosas conectadas en IOT tienden a ser diversas en términos de protocolos soportados, métodos y capacidades de comunicación, potencia computacional y almacenamiento. Esto nos motiva a buscar modelos de microservicio IoT más rentables y menos intensivos en recursos. Hemos identificado una amplia gama de disciplinas de diseño involucradas en la creación de sistemas IoT, que actúan como una interfaz transparente para colaborar con cosas heterogéneas, y adecuadas para ser implementadas en dispositivos con recursos limitados. Los patrones de IOT cubiertos en este documento varían en su granularidad y nivel de abstracción. Son artefactos de diseño interrelacionados y bien estructurados, que proporcionan soluciones eficientes y confiables a los problemas recurrentes descubiertos por los arquitectos del sistema IoT. Los autores ofrecen buenos consejos para diseñar, construir y escalar con interacciones entre dispositivos inherentes a ecosistemas complejos de IoT.},
   pages = {277-282},
   publisher = {IEEE},
   title = {IoT Design Patterns: Computational Constructs to Design, Build and Engineer Edge Applications},
   url = {http://ieeexplore.ieee.org/document/7471374/},
   year = {2016},
}
@inproceedings{Thiele2016,
   author = {Thomas Thiele and Thorsten Sommer and Sebastian Stiehm and Sabina Jeschke and Anja Richert},
   doi = {10.1109/W-FiCloud.2016.58},
   isbn = {978-1-5090-3946-3},
   journal = {2016 IEEE 4th International Conference on Future Internet of Things and Cloud Workshops (FiCloudW)},
   month = {8},
   note = {thiele2016<br>
    	La determinación de las sinergias en las redes de investigación a menudo se aborda mediante citas y análisis de coautoría utilizando publicaciones científicas. Aunque estos últimos métodos permiten una comprensión detallada de la cooperación científica y las comunidades, la mayor parte de los datos, el cuerpo de texto de la publicación, es mayormente ignorado. Por lo tanto, este cuerpo de texto contiene datos valiosos, que pueden ser procesados ​​para describir entidades en una red de investigación, detectar sinergias entre esas entidades y hacer uso de estas sinergias, p. En una mayor cooperación. Este trabajo tiene como objetivo la descripción de una arquitectura prototípica, que utiliza la ciencia de datos para detectar temas ocultos de los datos de publicación. Estos temas se utilizan para describir entidades (por ejemplo, institutos o proyectos) en una red de investigación. La proximidad tópica entre esas entidades se calcula mediante la aplicación de la clasificación. Con el fin de permitir a los actores explorar los resultados de estos procesos, la arquitectura genera automáticamente visualizaciones gráficas de las sinergias derivadas. Este documento contiene una descripción y definición de los elementos técnicos y la cadena de proceso de la arquitectura para derivar estas sinergias en los datos de la red. Esto se acompaña de un breve resumen de los resultados con el prototipo.},
   pages = {246-251},
   publisher = {IEEE},
   title = {Exploring Research Networks with Data Science: A Data-Driven Microservice Architecture for Synergy Detection},
   url = {http://ieeexplore.ieee.org/document/7592732/},
   year = {2016},
}
@inproceedings{Hassan2016,
   author = {Sara Hassan and Rami Bahsoon},
   doi = {10.1109/SCC.2016.113},
   isbn = {978-1-5090-2628-9},
   journal = {2016 IEEE International Conference on Services Computing (SCC)},
   month = {6},
   note = {hassan2016<br>
    	La migración a microservicios (microservitización) permite optimizar la autonomía, la sustituibilidad, la gobernanza descentralizada y la trazabilidad de las arquitecturas de software. A pesar del hype para la microservitización, el estado de la técnica aún carece de consenso sobre la definición de microservicios, sus propiedades y sus técnicas de modelado. Este artículo resume las opiniones de microservicios de la literatura informal para reflexionar sobre el contexto fundacional de este cambio de paradigma. Un contexto fundacional fuerte puede avanzar nuestra comprensión de la microservitización y ayudar a guiar a los arquitectos de software en el abordaje de sus problemas de diseño. Uno de estos problemas de diseño es la finalización del nivel óptimo de granularidad de una arquitectura de microservicio. Los compromisos de diseño relacionados incluyen: equilibrar el tamaño y el número de microservicios en una arquitectura y equilibrar los niveles de satisfacción de requisitos no funcionales de los microservicios individuales, así como su satisfacción para el sistema en general. Proponemos cómo la autoadaptación puede ayudar a abordar estos trade-offs de diseño y discutir algunos de los desafíos como una solución autoadaptativa. Utilizamos un hipotético sistema de streaming de películas en línea para motivar estos trade-offs de diseño. Se presenta una hoja de ruta de la solución en términos de las fases de un bucle de control de realimentación.},
   pages = {813-818},
   publisher = {IEEE},
   title = {Microservices and Their Design Trade-Offs: A Self-Adaptive Roadmap},
   url = {http://ieeexplore.ieee.org/document/7557535/},
   year = {2016},
}
@inproceedings{Gadea2016,
   author = {Cristian Gadea and Mircea Trifan and Dan Ionescu and Bogdan Ionescu},
   city = {New York, New York, USA},
   doi = {10.1145/2904111.2904115},
   isbn = {9781450342940},
   journal = {Proceedings of the 3rd Workshop on CrossCloud Infrastructures & Platforms - CrossCloud '16},
   note = {gadea2016<br>
    	Abstracto<br>Los frameworks web modernos y los proveedores de back-end como servicio hacen posible que las actualizaciones en tiempo real de un modelo de datos NoSQL se reflejen en las interfaces de usuario de múltiples aplicaciones de usuario final que se suscriben. Sin embargo, sigue siendo difícil para los usuarios descubrir dinámicamente y utilizar instantáneamente los datos proporcionados por la plétora de APIs REST existentes en varios proveedores de la nube hoy en día. Este artículo presenta una arquitectura de referencia basada en la idea de una base de datos NoSQL escalable que permite a múltiples suscriptores recibir notificaciones instantáneas de cambios en la base de datos mediante el uso de una "livequery". Al mantener una conexión WebSocket abierta entre cada navegador web de cliente y un Servidor de sincronización de objetos, este documento muestra cómo se pueden organizar y transmitir datos de múltiples API REST dispares a los clientes interesados ​​a través de la base de datos. Un ejemplo se ofrece con un editor de texto enriquecido colaborativo que utiliza un microservicio Named-Entity Recognition.},
   pages = {1-6},
   publisher = {ACM Press},
   title = {A reference architecture for real-time microservice API consumption},
   url = {http://dl.acm.org/citation.cfm?doid=2904111.2904115},
   year = {2016},
}
@article{Pozdniakova2017,
   author = {Olesia Pozdniakova and Dalius Mažeika},
   doi = {10.22364/bjmc.2017.5.1.08},
   issn = {22558942},
   issue = {1},
   journal = {Baltic Journal of Modern Computing},
   note = {5_1_08_Pozdniakova<br>
    	Muchas compañías están adoptando cloud para aplicaciones de hosting entregadas como un servicio a través de Internet. Un servicio de aplicación entregado de esta forma se denomina Software as a Service (SaaS). Los proveedores de SaaS obtienen beneficios de obtener recursos de cloud computing bajo demanda ya través de modelos de facturación de pago a medida. Estas capacidades permiten un desarrollo y despliegue de aplicaciones más rápido con una menor inversión inicial en la infraestructura. Para aprovechar al máximo la nube, la arquitectura del software debe diseñarse teniendo en cuenta que el software se ejecutará en la nube. Sin embargo, una aplicación heredada a menudo se desarrolla mediante el enfoque de arquitectura monolítica y no puede obtener todas las ventajas proporcionadas por una computación en la nube. Una aplicación monolítica creciente se vuelve menos flexible en el desarrollo, tiene más tiempo de aprovisionamiento, ralentiza la velocidad de la innovación y reduce la economía de escala. Como resultado de tales problemas, no sólo las empresas comerciales, sino también los diversos investigadores académicos buscan diseñar software como una aplicación "nativa de la nube". Este tipo de software tiene requisitos no funcionales específicos que definen escalabilidad, fiabilidad, tolerancia a fallos y otros requisitos específicos de la nube. Esta revisión sistemática de la literatura se basa en los estudios de casos, artículos publicados y otras publicaciones relacionadas con las aplicaciones en la nube o en la nube. Generaliza los rasgos comunes de los requisitos no funcionales definidos en los recursos analizados. Además, recorre los estilos arquitectónicos de las aplicaciones que se ejecutan en una nube y presenta arquitecturas desarrolladas recientemente que se utilizan para servicios de software a gran escala entregados en una nube},
   pages = {124-135},
   title = {Systematic Literature Review of the Cloud-ready Software Architecture},
   volume = {5},
   url = {http://www.bjmc.lu.lv/fileadmin/user_upload/lu_portal/projekti/bjmc/Contents/5_1_08_Pozdniakova.pdf},
   year = {2017},
}
@inproceedings{Klock2017,
   author = {Sander Klock and Jan Martijn E. M. Van Der Werf and Jan Pieter Guelen and Slinger Jansen},
   doi = {10.1109/ICSA.2017.38},
   isbn = {978-1-5090-5729-0},
   journal = {2017 IEEE International Conference on Software Architecture (ICSA)},
   month = {4},
   note = {klock2017<br><br>
    	En una arquitectura de microservicio, cada servicio está diseñado para ser independiente de otros microservicios. El tamaño de un microservicio, definido por las características que proporciona, afecta directamente su rendimiento y disponibilidad. Sin embargo, ninguno de los enfoques actualmente disponibles tiene esto en cuenta. Este documento propone un enfoque para mejorar el rendimiento de una arquitectura de microservicio por la carga de trabajo basado en la agrupación de características. Dado un modelo de la característica, la arquitectura actual del microservice, y la carga de trabajo, este acercamiento recomienda un despliegue que mejore el funcionamiento para la carga de trabajo dada usando un algoritmo genético. Creamos Micado, una herramienta de código abierto, en la que implementamos este enfoque, y lo aplicamos en un estudio de caso sobre un sistema ERP. Para diferentes cargas de trabajo, las arquitecturas de microservicio generadas resultantes muestran mejoras sustanciales, lo que establece el potencial del enfoque.},
   pages = {11-20},
   publisher = {IEEE},
   title = {Workload-Based Clustering of Coherent Feature Sets in Microservice Architectures},
   url = {http://ieeexplore.ieee.org/document/7930194/},
   year = {2017},
}
@inproceedings{Hassan2017,
   author = {Sara Hassan and Nour Ali and Rami Bahsoon},
   doi = {10.1109/ICSA.2017.32},
   isbn = {978-1-5090-5729-0},
   journal = {2017 IEEE International Conference on Software Architecture (ICSA)},
   month = {4},
   note = {hassan2017<br>
    	El aislamiento de las funciones empresariales de grano fino por fronteras en entidades denominadas microservicios es una actividad fundamental subyacente a la microservitización. Definimos la microservitización como el cambio de paradigma hacia microservicios. La determinación de los límites óptimos de los microservicios (es decir, la granularidad del microservicio) está entre las decisiones claves del diseño de la microservi- tización que influyen en la Calidad de Servicio (QoS) de la aplicación de microservicio en tiempo de ejecución. En este artículo, ofrecemos un enfoque arquitectónico para modelar este problema de decisión. Nos basamos en ambientes - un enfoque de modelado que puede capturar explícitamente fronteras funcionales y su adaptación. Ampliamos el enfoque de meta-modelado arquitectónico orientado a aspectos de ambientes - AMBIENT-PRISMA - con ambientes de microservicio. Un microservice ambient es un concepto de modelado que trata los límites del microservicio como una entidad adaptable de primera clase. Utilizamos un hipotético sistema basado en suscripción de películas en línea para capturar un escenario de microservitación usando nuestro enfoque de modelado orientado a aspectos. Los resultados muestran la capacidad de los ambientes de microservicio para expresar el límite funcional de un microservicio, las preocupaciones de cada frontera, las relaciones a través de los límites y las adaptaciones de estos límites. Además, evaluamos la expresividad y la efectividad de los ambientes de microservicio utilizando criterios de los marcos de clasificación de lenguaje de descripción de arquitectura (ADL), ya que los ambientes de microservicio esencialmente apoyan la descripción de arquitectura para microservicios. La evaluación se centra en las construcciones de modelado fundamentales de los ambientes de microservicio y en la forma en que apoyan las propiedades de la microservitización, tales como el diseño impulsado por las utilidades, la heterogeneidad de las herramientas y la gobernanza descentralizada. La evaluación pone de relieve cómo los ambientes de microservicio soportan el análisis, la evolución y la conciencia de localización / movilidad que son importantes para la adaptación de la granularidad de microservicios basada en la calidad. La evaluación es general e independientemente del dominio de la aplicación en particular y de las competencias empresariales en ese dominio.},
   pages = {1-10},
   publisher = {IEEE},
   title = {Microservice Ambients: An Architectural Meta-Modelling Approach for Microservice Granularity},
   url = {http://ieeexplore.ieee.org/document/7930193/},
   year = {2017},
}
