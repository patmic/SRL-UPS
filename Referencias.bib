@inproceedings{Hassan2016813,
   abstract = {Migrating to microservices (microservitization) enablesoptimising the autonomy, replaceability, decentralisedgovernance and traceability of software architectures. Despitethe hype for microservitization, the state of the art still lacksconsensus on the definition of microservices, their propertiesand their modelling techniques. This paper summarises viewsof microservices from informal literature to reflect on the foundationalcontext of this paradigm shift. A strong foundationalcontext can advance our understanding of microservitizationand help guide software architects in addressing its designproblems. One such design problem is finalising the optimallevel of granularity of a microservice architecture. Relateddesign trade-offs include: balancing the size and numberof microservices in an architecture and balancing the nonfunctionalrequirement satisfaction levels of the individualmicroservices as well as their satisfaction for the overall system. We propose how self-adaptivity can assist in addressing thesedesign trade-offs and discuss some of the challenges such a selfadaptivesolution. We use a hypothetical online movie streamingsystem to motivate these design trade-offs. A solution roadmapis presented in terms of the phases of a feedback control loop. © 2016 IEEE.},
   author = {S Hassan and R Bahsoon},
   doi = {10.1109/SCC.2016.113},
   journal = {Proceedings - 2016 IEEE International Conference on Services Computing, SCC 2016},
   note = {<b>Resumen</b>-La migración a microservicios (microservitización) permite
optimizar la autonomía, la sustituibilidad, la descentralización
gobernabilidad y trazabilidad de arquitecturas de software. A pesar de
el hype para la microservitización, el estado de la técnica todavía carece
consenso sobre la definición de microservicios, sus propiedades
y sus técnicas de modelado. Este documento resume las opiniones
de los microservicios de la literatura informal para reflexionar sobre
contexto de este cambio de paradigma. Una base sólida
contexto puede avanzar en nuestra comprensión de la microservi-
y ayudar a los arquitectos de software de guía en la dirección de su diseño
problemas. Uno de estos problemas de diseño es la finalización de la óptima
nivel de granularidad de una arquitectura de microservicio. Relacionado
los compromisos de diseño incluyen: equilibrar el tamaño y el número
de los microservicios en una arquitectura y el equilibrio de la no funcional
nivel de satisfacción de los requerimientos del individuo
microservicios, así como su satisfacción por el sistema global.
Proponemos cómo la autoadaptación puede ayudar a abordar estos
diseñar trade-offs y discutir algunos de los desafíos como un selfadaptive
solución. Utilizamos un hipotético streaming de películas en línea
sistema para motivar estos trade-offs del diseño. Una hoja de ruta de la solución
se presenta en términos de las fases de un bucle de control de realimentación.
    <br><br><b>CONCLUSIÓN
</b>Nuestra contribución se ha reflejado en la literatura informal
sobre microservicios, sus propiedades y su modelado
técnicas. Hemos formulado entonces el problema de encontrar
el nivel óptimo de granularidad durante la microservitización,
motivar el problema utilizando una hipotética película en línea
streaming sistema basado en suscripción. Rompemos el
problema en dos compensaciones: el tamaño / número de microservicios y
la satisfacción global / local NFR. Un tiempo de ejecución autoadaptable
solución al problema se propone y discute.
Algunos de los retos de investigación y aspectos prácticos que
microservitisation plantea a tal solución se destacan
junto con posibles direcciones futuras de investigación.<br><br>},
   pages = {813-818},
   title = {Microservices and their design trade-offs: A self-adaptive roadmap},
   url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84989952704&doi=10.1109%2FSCC.2016.113&partnerID=40&md5=9f19cdb66a2b79f1c63821348e8afa34},
   year = {2016},
}

